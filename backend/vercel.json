{
    "version": 2,
    "builds": [
      {
        "src": "src/server/app.ts",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server/app.ts",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ],
    "buildCommand": "npx prisma generate && tsc"
  }
  